{{ define "content" }}
<!-- Main -->
<div id="main" class="wrapper style1">
   <div class="container">
      <!-- Content -->
      <center>
        <section id="upper">
          <header class="">
            <div class="row">
              <div class="12u">
                <a href="/"><img src="/public/images/condor_logo.png" /></a>
              </div>
            </div>
          </header>
        </section>
      </center>
      <div>&nbsp;</div>
      <section class="box">
        <table class="table" id="results-table">
          <tr>
            <th class="results blank">&nbsp;</th>
          {{ range $index, $element := .Headers }}
              <th class="th-{{ $element }} results"> {{ $element }} </th>
          {{ end }}
          </tr>
          {{ range $index, $element := .Results }}
            <tr><td class="td-header header"> {{ $index }}</td>
            {{ range $key, $value := $element }}
                <td class="td-{{ $key }} results"> {{ $value }} </td>
            {{ end }}
            </tr>
          {{ end }}
        </table>
            </section>
          </div>
        </div>
{{ end }}
